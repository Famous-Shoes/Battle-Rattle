  <RecipeDef ParentName="BaseDroidRecipe">
    <defName>BattleRattle_Pouches_IFAK_PackRecipe</defName>
    <label>Pack IFAK</label>
    <description>
      Pack an IFAK with medical supplies.
    </description>
    
    <jobString>Packing IFAK</jobString>
    <workAmount>100</workAmount>
    <workSpeedStat>TailoringSpeed</workSpeedStat>
    <effectWorking>Tailor</effectWorking>
    <soundWorking>Recipe_Tailor</soundWorking>
    
    <ingredients>
			<li>
				<filter>
					<categories>
						<li>Medicine</li>
					</categories>
				</filter>
				<count>1</count>
			</li>
      <li>
        <filter>
          <thingDefs>
            <li>BattleRattle_Pouches_IFAK</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>MD2Circuitry</li>
          </thingDefs>
        </filter>
        <count>10</count>
      </li>
		</ingredients>
		<products>
      <MD2SpawnLogisticsDroid>1</MD2SpawnLogisticsDroid>
		</products>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Steel</li>
        <li>Plasteel</li>
        <li>MD2Circuitry</li>
			</thingDefs>
		</fixedIngredientFilter>
    <workSkill>Crafting</workSkill>
  </RecipeDef>

